
#include <math.h>

#include <stdio.h>

#include <string.h>

#include <stdlib.h>

#include <assert.h>

#include <limits.h>

#include <stdbool.h>




int main(){

    int n,count=0; 

    scanf("%d",&n);

    int arr[n];

    for(int arr_i = 0; arr_i < n; arr_i++){

       scanf("%d",&arr[arr_i]);

    }

    

   // printf("%d",min);

    //cutting function

   while(1)

       {

       int min=100;

    for(int arr_i = 0; arr_i < n; arr_i++){

        

       if(arr[arr_i]<min&&arr[arr_i]!=0)

           {

           min = arr[arr_i];

       }

    }

       //printf("mini %d",min);

    for(int arr_i = 0; arr_i < n; arr_i++){

       if(arr[arr_i]>0)

           {

           arr[arr_i]-=min;

           count++;

       }

    }

       if(count==0)

           {

           exit(0);

           }

    printf("%d\n",count);

       count=0;

  

   }

    return 0;

}
